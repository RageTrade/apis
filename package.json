{
  "name": "apis",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "serve": "pm2 start ./build/server.js",
    "serve:stop": "pm2 stop ./build/server.js",
    "serve:dev": "nodemon ./server.ts",
    "index": "pm2 start ./build/indexer/start.js",
    "index:stop": "pm2 stop ./build/indexer/start.js",
    "start": "yarn build && yarn start:nobuild",
    "start:nobuild": "yarn serve && yarn index",
    "restart": "yarn build && yarn stop && yarn start:nobuild",
    "restart2": "yarn stop && yarn build && yarn start:nobuild",
    "stop": "pm2 delete all 2>&1 || true"
  },
  "dependencies": {
    "@ragetrade/sdk": "^1.0.6",
    "@types/node-fetch": "^2.6.2",
    "@urql/devtools": "^2.0.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.1",
    "express": "~4.16.1",
    "fs-extra": "^10.1.0",
    "graphql": "^16.5.0",
    "http-errors": "~1.6.3",
    "ioredis": "^5.2.3",
    "isomorphic-unfetch": "^3.1.0",
    "morgan": "~1.9.1",
    "node-fetch": "2",
    "react": "^18.2.0",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "nodemon": "^2.0.19",
    "pm2": "^5.2.0",
    "ts-node": "^10.9.1",
    "typescript": "4.6"
  }
}
